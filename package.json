    "execa": "^6.1.0",
    "garfish": "^1.5.2",
    "minimist": "^1.2.6",
    "picocolors": "^1.0.0",
    "tsx": "^3.9.0",
    "typescript": "^4.8.3",
    "vue": "^3.2.39"
  },
  "devDependencies": {
    "@types/react": "^18.0.19"
  }
